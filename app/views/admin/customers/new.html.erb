<h2>新規利用者　入力</h2>

<%= form_with model: @customer, url: admin_customers_path, local: true do |f| %>
  <h5>氏名</h5>
    姓<%= f.text_field :first_name %>
    名<%= f.text_field :last_name %>

  <h5>フリガナ</h5>
    セイ<%= f.text_field :first_name_kana %>
    メイ<%= f.text_field :last_name_kana %>

  <%= f.submit "新規登録", class:"btn btn-primary col-xs-12" %>

<% end %>