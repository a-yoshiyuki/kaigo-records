<div class="container-fluid">
  <div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-3 col-sm-offset-1">
      <h2>利用者一覧</h2>
    </div>
    <div class="col-sm-4"></div>
  </div>

  <div class="row">
    <div class="col-sm-4"></div>
    <div class="col-sm-3 col-sm-offset-1">
      <div class="list">
        <% @customers.each do |customers| %>
          <ul>
            <%= link_to staff_member_customer_path(customers) do %>
              <li><%= customers.full_name %>様</li>
            <% end %>
          </ul>
        <% end %>
      </div>
    </div>
    <div class="col-sm-4"></div>
  </div>
</div>